<%= form_for transaction_group do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :regex %>
  <%= f.text_field :regex %>

  <% if transaction_group.persisted? %>
    <%= link_to('Slett', transaction_group_path(transaction_group), :method => :delete) %>
  <% end %>
  <%= f.submit %>
<% end %>
